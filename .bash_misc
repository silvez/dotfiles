# file with misc functions for bash
#
# cmd line intended to extract dynamips routers configs
function misc-rios()
{
    if [ -z $1 ]; then
        echo "especify a IOS config file..."
        return 1
    else
        for i in \
        $(sed -n 's/^.*configuration = \(.*$\)/\1\n/p' $1 2> /dev/null); do
            echo $i | base64 -d
        done
    fi
}
